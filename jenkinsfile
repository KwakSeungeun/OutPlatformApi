pipeline {
  agent none
  environment {
    CI = 'true'
  }
  stages {
    stage('Build') {
      steps {
        sh 'npm install'
      }
    }
    stage('Test') {
      when {
        branch 'develop'
      }
      steps {
        sh 'npm install'
        stash includes: '**', name: 'App' 
      }
    }
    stage('Develop') {
      when {
        branch 'develop'
      }
      steps {
        echo 'Develop Environment'
      }
    }
  }
}
pipeline {
  agent none 
  environment {
    CI = 'true'
  }
  stages {
    stage('Develop') {
      agent {
        node {
          label 'develop'
          customWorkspace '/develop'
        }
      }
      when {
        branch 'develop'
      }
      steps {
        // stash includes: '**/target/*.jar', name: 'app'
        // docker connection
        sh 'docker exec -i develop bash'
        // sleep for connetion delay
        sleep 1
        // 
        echo "develop docker"
        // git checkout
      }
    }
  }
}